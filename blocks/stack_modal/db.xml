<?xml version="1.0"?>
<schema version="0.3">
	<table name="btStackModal">
		<field name="bID" type="I">
			<key />
			<unsigned />
		</field>
		<field name="linkType" type="C" size="5">
			<default value="text" />
		</field>
		<field name="linkContent" type="C" size="128">
			<default value="Launch Modal" />
		</field>
		<field name="width" type="I" size="4">
			<unsigned />
		</field>
		<field name="height" type="I" size="4">
			<unsigned />
		</field>
		<field name="stack" type="I" size="5">
	    </field>
		<field name="title" type="C" size="128">
		</field>
	</table>
</schema>